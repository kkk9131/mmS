# 開発環境設定例
# このファイルをコピーして .env.development を作成し、実際の値を設定してください

# Supabase設定（開発環境）
EXPO_PUBLIC_SUPABASE_URL=https://your-dev-project-id.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-dev-anon-key-here

# 開発用サービスロールキー（デバッグ用）
SUPABASE_SERVICE_ROLE_KEY=your-dev-service-role-key-here

# セキュリティ設定（開発用に緩和）
ENABLE_RLS=true
REQUIRE_AUTH=true
MAX_LOGIN_ATTEMPTS=10
SESSION_TIMEOUT=604800000

# パフォーマンス設定
SUPABASE_MAX_CONNECTIONS=5
SUPABASE_TIMEOUT=10000
SUPABASE_RETRY_ATTEMPTS=1

# キャッシュ設定
ENABLE_CACHING=true
CACHE_TIMEOUT=60000
MAX_CACHE_SIZE=50
BATCH_SIZE=10

# 監視設定（デバッグ用）
LOG_LEVEL=debug
ENABLE_ERROR_REPORTING=true
ENABLE_PERFORMANCE_TRACKING=true
ENABLE_USER_ANALYTICS=true

# 機能フラグ
ENABLE_REALTIME=true
ENABLE_PUSH_NOTIFICATIONS=false
ENABLE_OFFLINE_MODE=true
ENABLE_IMAGE_UPLOAD=true

# 開発環境識別
NODE_ENV=development

# 開発用ポート設定
EXPO_DEV_SERVER_PORT=3048